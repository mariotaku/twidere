<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="Twidere">
		<attr name="switchStyle" format="reference"/>
		<attr name="editTextStyle" format="reference"/>
		<attr name="preferenceTitleStyle" format="reference"/>
	</declare-styleable>
	<declare-styleable name="ViewPagerIndicator">
		<attr name="tabStyle" format="reference"/>
		<attr name="tabTextStyle" format="reference"/>
		<attr name="tabBackground" format="reference"/>
	</declare-styleable>
	<declare-styleable name="Switch">
		<attr name="thumb" format="reference"/>
		<attr name="track" format="reference"/>
		<attr name="textOn" format="reference|string"/>
		<attr name="textOff" format="reference|string"/>
		<attr name="thumbTextPadding" format="dimension"/>
		<attr name="switchTextAppearance" format="reference"/>
		<attr name="switchMinWidth" format="dimension"/>
		<attr name="switchPadding" format="dimension"/>
	</declare-styleable>
	<declare-styleable name="TextAppearance">
		<attr name="textColor" format="reference"/>
		<attr name="textSize" format="dimension"/>
	</declare-styleable>

</resources>